package learning;

public class Queue {
	private Che font;
	private Che rear;
	private int size;
	public Queue(Che data){
		this.data=data;
		font=null;
		rear=null;
		size=0;
	}
	public boolean isEmpty(){
		if(size==0){
			return true;
		}
		return false;
	}
	public boolean isFull(int n){
		if(size==n) return true;
		return false;
	}
	public boolean push(Che x,int n){
		if(isFull(n)){
			return false;
		}
		x.setLast(rear);
		rear.setNext(x);
		rear=x;
		size++;
		return true;
	}
	
	
	
	
	
	
	
	
	
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public Che getFont() {
		return font;
	}
	public void setFont(Che font) {
		this.font = font;
	}
	public Che getRear() {
		return rear;
	}
	public void setRear(Che rear) {
		this.rear = rear;
	}
}
