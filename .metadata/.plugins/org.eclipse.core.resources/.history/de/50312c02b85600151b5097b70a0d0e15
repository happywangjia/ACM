package ahu;

import java.util.Scanner;

/**
 * 
 * @tag:配对
 * @Num:N_711.java
 * @author hijj
 * Create at: 2015年9月8日 下午12:54:27
 */
public class N_711 {
	public final static int MOD=(int)(1e9+7+0.5);
	public static int x,y;
	public static void main(String[] args){
		Scanner cin=new Scanner(System.in);
		int n=0;
		while(++n<1000001){
			
			//int n=cin.nextInt();
			Long mod1=(long) 1;
			int z,qq=n;
			for(int i=n+1;i<=2*n;i++){
				z=i;
				while(qq!=0&&z%2==0){
					z/=2;
					qq--;
				}
				mod1=mod1*z%MOD;
			}
			if(qq!=0)
				System.out.println("wangjia"+n);
			//System.out.println(mod1);
		}
		
	}
}
