package leetcode;

public class N5_longest {
	public String longestPalindrome(String s) {
		int len=s.length();
		int max=0,maxMiddle,j;
		for(int i=1;i<len;i++){
			int tmp=0;
			for(j=1;j<=i&&i+j<len;j++){
				if(s.charAt(i-j)!=s.charAt(i+j))
					break;
			}
			tmp=j-1;
			if(max<j){
				max=j;
				maxMiddle=i;
			}
		}
		
		
		return null;
	}
}
