package learning;

import java.io.IOException;

import algorithms.N_004;

public class learn_2 {
	private char[] ch={'+','-','*','/','^','(',')','#'};
	private int[] f1={4,4,6,6,7,2,9,1};
	private int[] f2={3,3,5,5,8,9,2,1};
	
	
	public static void main(String[] args) throws IOException{
		System.out.println("请输入表达式，以'#'结束：");
		boolean digFlag=false;
		char ch;
		int sum=0;
		N_004<Character> mStack=new N_004<Character>();
		ch = (char) System.in.read();
		while(true){
			if(ch<='9'&&ch>='0'){
				digFlag=true;
				mStack.push(ch);
			}else if(digFlag&&isFu(ch)){
				sum=0;
				
			}else if(ch=='#'){
				
			}else{
				System.out.println("输入错误！");
				break;
			}
		}	
	}
	private static boolean isFu(char ch){
		char[] fu={'+','-','*','/','^','(',')'};
		for(int i=0;i<7;i++){
			if(fu[i]==ch) return true;
		}
		return false;
	}

}
