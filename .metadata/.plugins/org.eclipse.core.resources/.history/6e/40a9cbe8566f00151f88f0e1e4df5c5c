package leetcode;

import java.util.HashMap;
import java.util.Map;

public class N13_Roman {
	public int romanToInt(String s) {
		Map<String,Integer> map=new HashMap<>();
		map.put("M", 1000);
		map.put("D", 500);
		map.put("CM", 900);
		map.put("CD", 400);
		map.put("C", 100);
		map.put("L",50);
		map.put("XC", 90);
		map.put("XL", 40);
		map.put("X",10);
		map.put("V", 5);
		map.put("IX", 9);
		map.put("IV", 4);
		
		return 0;
	}
}
