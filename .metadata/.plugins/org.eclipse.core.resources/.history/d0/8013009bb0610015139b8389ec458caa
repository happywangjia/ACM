package learning;

public class Che {
	private String hao;
	private int jkTime;
	private int ckTime;
	private int jdTime;
	private int cdTime;
	private boolean hFlag;
	public String getHao() {
		return hao;
	}
	public void setHao(String hao) {
		this.hao = hao;
	}
	public int getJkTime() {
		return jkTime;
	}
	public void setJkTime(int jkTime) {
		this.jkTime = jkTime;
	}
	public int getCkTime() {
		return ckTime;
	}
	public void setCkTime(int ckTime) {
		this.ckTime = ckTime;
	}
	public int getJdTime() {
		return jdTime;
	}
	public void setJdTime(int jdTime) {
		this.jdTime = jdTime;
	}
	public int getCdTime() {
		return cdTime;
	}
	public void setCdTime(int cdTime) {
		this.cdTime = cdTime;
	}
	public boolean ishFlag() {
		return hFlag;
	}
	public void sethFlag(boolean hFlag) {
		this.hFlag = hFlag;
	}
	
}
