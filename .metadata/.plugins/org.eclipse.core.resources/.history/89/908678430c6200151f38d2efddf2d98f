package learning;

public class Stack {
	private Che[] shu=new Che[100];
	private int size;
	
	public Stack(){
		size=0;
	}
	public boolean isEmpty(){
		if(size==0) return true;
		return false;
	}
	public boolean isFull(int n){
		if(n==size) return true;
		return false;
	}
	
	public boolean push(Che x,int n){
		if(isFull(n)) return false;
		shu[size]=x;
		size++;
		return true;
	}
	public Che pop(){
		if(isEmpty()) return null;
		size--;
		return shu[size];
	}
	
	public int getSize() {
		return size;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public Che getTop() {
		return shu[size-1];
	}
}
