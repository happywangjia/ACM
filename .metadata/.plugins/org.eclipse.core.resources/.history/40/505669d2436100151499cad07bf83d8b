package leetcode;

import java.util.Scanner;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class N10_regular {
	public static boolean isMatch(String s, String p) {
		int len1 = s.length();
		int len2 = p.length();
		int d=0;
		for(int i=0;i<len2&&d<len1;i++){
			if(i!=0&&p.charAt(i)=='*'){
				char c=p.charAt(i-1);
				while(d<len1){
					if(s.charAt(d)==c){
						d++;
					}
				}
				if(d==len1) return true;
				while(p.charAt(i+1)==c)
					i++;
				continue;
			}
			if(p.charAt(i)=='.'||p.charAt(i)==s.charAt(d)){
				for(int j=0;j<=i;j++){
					if(p.charAt(j)=='.'){
						p.replace('.', s.charAt(d));
					}
				}
				d++;
				if(d==len1) return true;
				continue;
			}
		}
		return false;
	}

	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		while (true) {
			String str1 = cin.nextLine();
			String str2 = cin.nextLine();
			boolean flag = isMatch(str1, str2);
			System.out.println(flag);
		}

	}
}
