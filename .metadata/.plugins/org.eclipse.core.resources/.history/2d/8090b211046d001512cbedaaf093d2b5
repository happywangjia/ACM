package leetcode;

import java.util.Scanner;

public class N10_regular {
	public static boolean isMatch(String s, String p) {
		int sLen=s.length();
		int pLen=p.length();
		if(sLen==0&&pLen==0) return true;
		if(sLen==0||pLen==0) return false;
		
		if(s.charAt(0)==p.charAt(0)){
			if(pLen>=2&&p.charAt(1)=='*'){
				if(isMatch(s, p.substring(2))) return true;
				int pk=1,sk=1;
				while(sk<sLen&&s.charAt(sk)==s.charAt(0)){
					sk++;
				}
				while(pk<pLen&&(p.charAt(pk)=='*'||p.charAt(pk)==p.charAt(0))){
					if(p.charAt(pk)==p.charAt(0)){
						if(isMatch(s.substring(sk), p.substring(pk))) return true;
						pk++;
					}
				}
				return isMatch(s.substring(sk), p.substring(pk));
			}
			if(p.charAt(0)=='.'){
				return isMatch(s.substring(1), p.substring(1));
			}
			if(p.charAt(0)=='*'){
				int pk=0,sk=0;
				if(isMatch(s, p.substring(1))) return true;
				while(sk<sLen&&s.charAt(sk)==s.charAt(0)){
					sk++;
				}
				if(isMatch(s.substring(sk), p.substring(1)));
				while(pk<pLen&&(p.charAt(pk)=='*'||p.charAt(pk)==s.charAt(0))){
					if(p.charAt(pk)==p.charAt(0)){
						if(isMatch(s.substring(sk), p.substring(pk))){
							return true;
						}
						pk++;
					}
				}
				return isMatch(s.substring(sk), p.substring(pk));
			}
		}
		
		
		
		return false;
	}

	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		while (true) {
			String str1 = cin.next();
			String str2 = cin.next();
			boolean flag = isMatch(str1, str2);
			System.out.println(flag);
		}
	}
}
