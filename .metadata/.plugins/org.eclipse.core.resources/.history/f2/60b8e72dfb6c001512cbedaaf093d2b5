package leetcode;

import java.util.Scanner;

public class N10_regular {
	public static boolean isMatch(String s, String p) {
		int sLen=s.length();
		int pLen=p.length();
		if(sLen==0&&pLen==0) return true;
		if(pLen==0) return false;
		if(s.charAt(0)==p.charAt(0)){
			int pk=1,sk=1;
			if(p.charAt(1)=='*'){
				while(p.charAt(pk)=='*'||p.charAt(pk)==p.charAt(0)){
					pk++;
				}
				while(s.charAt(sk)==s.charAt(0)){
					sk++;
				}
				return isMatch(s.substring(sk), p.substring(pk));
			}
			return isMatch(s.substring(1), s.substring(1));
		}
		if(p.charAt(0)=='.'){
			return isMatch(s.substring(1), p.substring(1));
		}
		if(p.charAt(0)=='*'){
			int pk=0,sk=1;
			while(p.charAt(pk)=='*'||p.charAt(pk)==s.charAt(0)){
				pk++;
			}
			while(s.charAt(sk)==s.charAt(0)){
				sk++;
			}
			return isMatch(s.substring(sk), p.substring(pk));
		}
		return false;
	}

	public static void main(String[] args) {
		Scanner cin = new Scanner(System.in);
		cin.close();
		while (true) {
			String str1 = cin.nextLine();
			String str2 = cin.nextLine();
			boolean flag = isMatch(str1, str2);
			System.out.println(flag);
		}
	}
}
